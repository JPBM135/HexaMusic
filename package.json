{
	"name": "@jpbm135/hexa-music",
	"version": "1.0.0",
	"description": "",
	"scripts": {
		"build:clean": "del-cli dist",
		"build:check": "tsc --noEmit",
		"build:esm": "swc ./src --out-dir ./dist",
		"build:locales": "cpy locales/** ./dist",
		"build": "yarn build:clean && yarn build:check && yarn build:esm && yarn build:locales",
		"lint": "prettier --check . && eslint src --ext ts",
		"format": "prettier --write . && eslint src --ext ts --fix",
		"fmt": "yarn format",
		"start": "env-cmd node --enable-source-maps dist/index.js",
		"start:dev": "yarn build && yarn dev",
		"dev": "env-cmd node --enable-source-maps dist/index.js",
		"update": "yarn upgrade-interactive",
		"server:update": "git pull && yarn --immutable && yarn build && pm2 restart",
		"postinstall": "patch-package",
		"migrate-up": "env-cmd ley up",
		"migrate-down": "env-cmd ley down",
		"migrate-create": "env-cmd ley new",
		"update-db-types": "pg-to-ts generate -c postgresql://postgres:test@127.0.0.1:7867/hexamusic -o src/@types/db-schema.ts",
		"reset-db": "env-cmd node bin/reset-db.mjs && yarn migrate-up"
	},
	"type": "module",
	"keywords": [],
	"author": "JPBM135 <jpedrobm0@gmail.com>",
	"license": "AGPL-3.0",
	"private": true,
	"dependencies": {
		"@discordjs/opus": "^0.9.0",
		"@discordjs/voice": "^0.16.1",
		"@sapphire/snowflake": "^3.5.2",
		"@types/spotify-api": "^0.0.25",
		"@yuudachi/framework": "^0.3.5",
		"bufferutil": "^4.0.8",
		"discord-ytdl-core": "^5.0.4",
		"discord.js": "^14.14.1",
		"erlpack": "^0.1.4",
		"express": "^4.18.2",
		"ffmpeg": "^0.0.4",
		"ffmpeg-static": "^5.2.0",
		"i18next": "^23.7.12",
		"i18next-fs-backend": "^2.3.1",
		"kleur": "^4.1.5",
		"knex": "^3.1.0",
		"ley": "^0.8.1",
		"node-gyp": "^10.0.1",
		"patch-package": "^6.5.1",
		"postgres": "^3.4.3",
		"prism-media": "^1.3.5",
		"prom-client": "^15.1.0",
		"reflect-metadata": "^0.1.14",
		"sodium-native": "^4.0.4",
		"spotify-web-api-node": "^5.0.2",
		"tsyringe": "^4.8.0",
		"undici": "^5.28.2",
		"utf-8-validate": "^6.0.3",
		"yarn": "^1.22.21",
		"youtube-ext": "^1.1.16",
		"youtube-sr": "^4.3.10",
		"ytdl-core": "^4.11.5",
		"zlib-sync": "^0.1.9"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.63",
		"@swc/core": "^1.3.101",
		"@types/express": "^4.17.21",
		"@types/knex": "^0.16.1",
		"@types/node": "^18.19.3",
		"@types/spotify-web-api-node": "^5.0.11",
		"cpy-cli": "^5.0.0",
		"del-cli": "^5.1.0",
		"env-cmd": "^10.1.0",
		"eslint": "^8.56.0",
		"eslint-config-neon": "^0.1.57",
		"pg-to-ts": "^4.1.1",
		"prettier": "^3.1.1",
		"typescript": "^5.3.3"
	},
	"engines": {
		"node": ">=17.9.0"
	},
	"packageManager": "yarn@3.2.2",
	"resolutions": {
		"ytdl-core@4.11.3": "patch:ytdl-core@npm:4.11.3#.yarn/patches/ytdl-core-npm-4.11.3-8bba1af4c9.patch"
	}
}